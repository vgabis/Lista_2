programa {
  funcao inicio() {
    
     real num1, num2, soma, sub, mult
    

    escreva ("Digite um número:  ")
    leia (num1)
      escreva ("Digite um número:  ")
    leia (num2)

    soma = num1 + num2
    sub = num1 - num2
